{
  "parser": "babel-eslint",
  "plugins": [
    "flowtype",
    "react"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
    "no-underscore-dangle": 0,
    "react/forbid-prop-types": 0,
    "max-len": 0,
    "no-console": 0,
    "object-curly-newline": 0
  }
}